<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.epicfitproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainMenuActivity" />
        <activity android:name=".ActivitiesActivity" />
        <activity android:name=".GoalActivity" />
        <activity android:name=".AthleteActivity" />
        <activity android:name=".ExerciseActivity">
            <uses-permission android:name="android.permission.INTERNET" />
        </activity>
        <activity android:name=".Statistics" />
        <activity android:name=".ListOFExercisesActivity" />
        <activity android:name=".AddExerciseProperties"></activity>
    </application>

</manifest>